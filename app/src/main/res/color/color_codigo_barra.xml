<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:color="#2262B9DF">
        <shape android:shape="rectangle">
            <solid android:color="#3388cc"/> <!-- Cor quando pressionado -->
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="#0077aa"/>
        </shape>
    </item>
    <item android:color="#2262B9DF">
        <shape android:shape="rectangle">
